function result=runDFS(status,R,x,w)


nQuad=status.nQuad;

[result.offset_calculated,result.std_offset_calculated]=lscov(R,x,w);

result.offset_calculated.qoffset=result.offset_calculated(1:nQuad);
result.offset_calculated.bpmoffset=result.offset_calculated(nQuad+1:end);
result.std_offset_calculated.std_qoffset=std_offset_calculated(1:nQuad);
result.std_offset_calculated.std_bpmoffset=std_offset_calculated(nQuad+1:end);
